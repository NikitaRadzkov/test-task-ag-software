{
  "plugins": [
    "prettier",
    "unicorn"
  ],
  "extends": [
    "airbnb-base",
    "plugin:unicorn/recommended",
    "plugin:prettier/recommended",
    "prettier",
    "prettier/unicorn"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "unicorn/prevent-abbreviations": "off",
    "max-len": ["warn", { "code": 120 }],
    "no-debugger": "off",
    "no-console": 0
  }
}